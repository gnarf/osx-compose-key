        "$\UF703" = ("insertText:", "→"); /* (Shift) Right arrow */
        "$~\UF700" = ("insertText:", "⇈"); /* (Shift-opt) Double-up arrow */
        "$~\UF701" = ("insertText:", "⇊"); /* (Shift-opt) Double-down arrow */
